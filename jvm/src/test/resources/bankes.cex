urn:cts:greekLit:tlg0012.tlg001.bankes:24.520#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="520" part="N">ανδρος ες οφθαλμους ὅς τοι πολέας τε και εσθλους</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.521#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="521" part="N">ῡϊέας <choice> <sic>εξεναριξε</sic> <corr instant="false">εξεναριξα</corr> </choice>· <w part="N">σιδή<gap extent="1" instant="false" unit="letters"/>ειον</w> νύ τοι ῆτορ·</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.522#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="522" part="N">αλλ αγε δη κατ άρ ἑζευ <gap extent="1" instant="false" unit="letters"/>πι θρόνου· <choice> <sic>αλλα γε</sic> <corr instant="false"><w part="N">αλγε<add instant="false" place="supralinear" status="unremarkable">α</add></w></corr> </choice> δ᾽ έμπης</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.523#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="523" part="N">εν θῡμῳ κατακεῖσθα<gap extent="1" instant="false" unit="letters"/> εᾱσομεν αχνύμενοί περ·</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.524#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="524" part="N">ου γάρ τις πρῆξις πέλεται κρυεροῖο γόοιο·</l>
