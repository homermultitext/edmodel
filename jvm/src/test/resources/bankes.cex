urn:cts:greekLit:tlg0012.tlg001.bankes:24.274#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="274" part="N">ἑξειης κατέδησαν· υπο γλωχείνα δε γνάμψαν·</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.275#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="275" part="N">εκ <w part="N"><unclear instant="false">θ</unclear>αλαμου</w> δε φεροντες εϋξεστης επ απηνης</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.276#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="276" part="N">νήεον <persName full="yes" instant="false" n="urn:cite2:hmt:pers:pers172">Ἑκτορεης</persName> κεφαλης απερείσι᾽ αποινα·</l>
