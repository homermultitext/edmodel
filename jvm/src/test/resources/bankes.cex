urn:cts:greekLit:tlg0012.tlg001.bankes:24.282#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="282" part="N"><w part="N">κ<unclear instant="false">η</unclear><gap extent="3" instant="false" unit="letters"/></w> και <persName full="yes" instant="false" n="urn:cite2:hmt:pers:pers145">Πρίαμος</persName>. πυκινα φρὲσι μήδε᾽ εχοντες</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.283#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="283" part="N"><w part="N">α<unclear instant="false">γ</unclear>χ<unclear instant="false">ιμ</unclear>ολον</w> δέ σφ ηλθ᾽ <persName full="yes" instant="false" n="urn:cite2:hmt:pers:pers535">Ἑκαβη</persName> τετιηότι θῡμῳ</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.284#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="284" part="N">οἰνον᾽ εχους εν χειρι μελιφρονα δεξιτερῇφι·</l>
urn:cts:greekLit:tlg0012.tlg001.bankes:24.285#<l xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" n="285" part="N">χρῡσεω εν δέπαι όφρα᾽ λείψαντε <w part="N">κ<unclear instant="false">ι</unclear>οίτην</w>·</l>
